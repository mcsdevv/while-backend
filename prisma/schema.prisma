generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id
  email     String   @default("")
  name      String   @default("")
  picture   String   @default("")
  updatedAt DateTime @default(now()) @updatedAt @map("updated_at")
}

model Contact {
  id        String   @id
  email     String   @default("")
  name      String   @default("")
  picture   String   @default("")
  company   String   @default("")
  position  String   @default("")
  updatedAt DateTime @default(now()) @updatedAt @map("updated_at")
}

model Meeting {
  id        String   @id
  updatedAt DateTime @default(now()) @updatedAt @map("updated_at")
}

model Agenda {
  id        String   @id
  updatedAt DateTime @default(now()) @updatedAt @map("updated_at")
}

model Note {
  id        String   @id
  updatedAt DateTime @default(now()) @updatedAt @map("updated_at")
}

model Task {
  id        String   @id
  updatedAt DateTime @default(now()) @updatedAt @map("updated_at")
}
